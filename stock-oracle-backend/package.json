{
    "name": "stock-oracle-backend",
    "version": "1.0.0",
    "description": "AI-powered warehouse management agent with RAG + pgvector",
    "prisma": {
      "schema": "src/db/schema.prisma"
    },
    "main": "dist/index.js",
    "scripts": {
      "dev": "tsx watch src/index.ts",
      "build": "tsc",
      "start": "node dist/index.js",
      "db:generate": "prisma generate",
      "db:migrate": "prisma migrate dev",
      "db:push": "prisma db push",
      "db:seed": "tsx src/db/seed.ts",
      "setup": "npm run db:push && npm run db:generate && tsx scripts/setup-db.ts && npm run db:seed",
      "embeddings": "tsx scripts/seed-embeddings.ts",
      "process-m5": "tsx scripts/process-m5-data.ts",
      "train-lstm": "tsx scripts/train-lstm.ts",
      "test": "tsx scripts/test-agent.ts",
      "test-forecast": "tsx scripts/test-forecast.ts",
      "lint": "eslint src --ext .ts"
    },
    "dependencies": {
      "@prisma/client": "^5.22.0",
      "express": "^4.18.2",
      "socket.io": "^4.7.2",
      "cors": "^2.8.5",
      "dotenv": "^16.3.1",
      "zod": "^3.22.4",
      "jsonwebtoken": "^9.0.2",
      "bcrypt": "^5.1.1",
      "langchain": "^0.3.5",
      "@langchain/openai": "^0.3.11",
      "@langchain/anthropic": "^0.3.5",
      "@langchain/community": "^0.3.11",
      "pgvector": "^0.2.0",
      "pg": "^8.11.3",
      "winston": "^3.11.0",
      "helmet": "^7.1.0",
      "express-rate-limit": "^7.1.5",
      "uuid": "^9.0.1",
      "node-cron": "^3.0.3",
      "@tensorflow/tfjs-node": "^4.14.0",
      "papaparse": "^5.4.1",
      "mathjs": "^12.2.0",
      "lodash": "^4.17.21"
    },
    "devDependencies": {
      "@types/express": "^4.17.21",
      "@types/node": "^20.10.5",
      "@types/cors": "^2.8.17",
      "@types/jsonwebtoken": "^9.0.5",
      "@types/bcrypt": "^5.0.2",
      "@types/uuid": "^9.0.7",
      "@types/pg": "^8.10.9",
      "@types/node-cron": "^3.0.11",
      "@types/papaparse": "^5.3.14",
      "@types/lodash": "^4.14.202",
      "prisma": "^5.22.0",
      "tsx": "^4.7.0",
      "typescript": "^5.3.3",
      "eslint": "^8.56.0",
      "@typescript-eslint/eslint-plugin": "^6.15.0",
      "@typescript-eslint/parser": "^6.15.0"
    }
  }